{
	"name": "@neovici/cosmoz-omnitable-treenode-column",
	"version": "7.1.3",
	"description": "A column for cosmoz-omnitable that displays a tree node.",
	"keywords": [
		"polymer",
		"web-components"
	],
	"homepage": "https://github.com/neovici/cosmoz-omnitable-treenode-column#readme",
	"bugs": {
		"url": "https://github.com/neovici/cosmoz-omnitable-treenode-column/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/neovici/cosmoz-omnitable-treenode-column.git"
	},
	"license": "Apache-2.0",
	"author": "",
	"main": "cosmoz-omnitable-treenode-column.js",
	"directories": {
		"test": "test"
	},
	"scripts": {
		"lint": "eslint --cache --ext .js .",
		"start": "npm run storybook",
		"test": "wtr --coverage",
		"test:watch": "wtr --watch",
		"storybook": "wds",
		"storybook:build": "build-storybook",
		"storybook:deploy": "storybook-to-ghpages",
		"prepare": "husky install"
	},
	"release": {
		"plugins": [
			"@semantic-release/commit-analyzer",
			"@semantic-release/release-notes-generator",
			"@semantic-release/changelog",
			"@semantic-release/github",
			"@semantic-release/npm",
			"@semantic-release/git"
		],
		"branch": "master",
		"preset": "conventionalcommits"
	},
	"publishConfig": {
		"access": "public"
	},
	"files": [
		"*.js"
	],
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"dependencies": {
		"@neovici/cosmoz-autocomplete": "^5.0.0",
		"@neovici/cosmoz-omnitable":  "^11.0.0 || ^12.0.0",
		"@neovici/cosmoz-tree": "^3.0.0",
		"@neovici/cosmoz-treenode": "^5.0.0",
		"@neovici/cosmoz-utils": "^5.16.0",
		"@polymer/paper-spinner": "^3.0.0",
		"@polymer/polymer": "^3.4.1",
		"lit-html": "^2.0.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.0.0",
		"@commitlint/config-conventional": "^17.0.0",
		"@neovici/cfg": "^1.15.2",
		"@open-wc/testing": "^3.0.0",
		"@semantic-release/changelog": "^6.0.0",
		"@semantic-release/git": "^10.0.0",
		"@storybook/storybook-deployer": "^2.8.7",
		"@web/dev-server": "^0.1.10",
		"@web/dev-server-storybook": "^0.5.0",
		"@webcomponents/webcomponentsjs": "^2.5.0",
		"eslint-import-resolver-typescript": "^3.2.7",
		"husky": "^8.0.0",
		"semantic-release": "^19.0.0",
		"sinon": "^14.0.0"
	}
}
