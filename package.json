{
	"name": "@neovici/cosmoz-omnitable-treenode-column",
	"version": "3.1.2",
	"description": "A column for cosmoz-omnitable that displays a tree node.",
	"keywords": [
		"polymer",
		"web-components"
	],
	"homepage": "https://github.com/neovici/cosmoz-omnitable-treenode-column#readme",
	"bugs": {
		"url": "https://github.com/neovici/cosmoz-omnitable-treenode-column/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/neovici/cosmoz-omnitable-treenode-column.git"
	},
	"license": "Apache-2.0",
	"author": "",
	"main": "cosmoz-omnitable-treenode-column.js",
	"directories": {
		"test": "test"
	},
	"scripts": {
		"lint": "eslint --cache --ext .js .",
		"start": "npm run storybook",
		"test": "karma start --coverage",
		"test:watch": "karma start --auto-watch=true --single-run=false",
		"test:update-snapshots": "karma start --update-snapshots",
		"test:prune-snapshots": "karma start --prune-snapshots",
		"test:compatibility": "karma start --compatibility all --auto-watch=true --single-run=false",
		"storybook": "start-storybook --node-resolve --watch --open",
		"storybook:build": "build-storybook",
		"storybook:deploy": "storybook-to-ghpages"
	},
	"release": {
		"plugins": [
			"@semantic-release/commit-analyzer",
			"@semantic-release/release-notes-generator",
			"@semantic-release/changelog",
			"@semantic-release/github",
			"@semantic-release/npm",
			"@semantic-release/git"
		],
		"branch": "master",
		"preset": "conventionalcommits"
	},
	"publishConfig": {
		"access": "public"
	},
	"files": [
		"*.js"
	],
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"husky": {
		"hooks": {
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
		}
	},
	"dependencies": {
		"@neovici/cosmoz-omnitable": "^3.7.0",
		"@neovici/cosmoz-tree": "^3.0.0",
		"@neovici/cosmoz-treenode": "^4.0.0",
		"@neovici/paper-autocomplete": "^4.0.0",
		"@polymer/paper-spinner": "^3.0.0",
		"@polymer/polymer": "^3.0.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^8.3.0",
		"@commitlint/config-conventional": "^8.3.0",
		"@neovici/eslint-config": "^1.2.0",
		"@open-wc/demoing-storybook": "^2.1.0",
		"@open-wc/testing": "^2.5.8",
		"@open-wc/testing-karma": "^3.3.10",
		"@semantic-release/changelog": "^5.0.0",
		"@semantic-release/git": "^9.0.0",
		"@storybook/storybook-deployer": "^2.8.0",
		"@webcomponents/webcomponentsjs": "^2.4.0",
		"babel-eslint": "^10.0.0",
		"chai": "^4.2.0",
		"deepmerge": "^4.2.0",
		"eslint": "^6.8.0",
		"eslint-plugin-html": "^6.0.0",
		"eslint-plugin-import": "^2.20.0",
		"eslint-plugin-mocha": "^6.3.0",
		"husky": "^4.2.0",
		"karma": "^5.0.0",
		"karma-firefox-launcher": "^1.3.0",
		"karma-sauce-launcher": "^4.0.0",
		"semantic-release": "^17.0.0",
		"sinon": "^9.0.0"
	}
}
